{
    "name": "AzureNotificationExtension",
    "description": "Chrome platform app.",
    "manifest_version": 2,
    "version": "0.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "gcm",
        "storage",
        "notifications",
        "https://*.servicebus.windows.net/*"
    ],
    "icons": {
        "128": "icons/logo.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}